<img src="http://www.clipartroo.com/images/75/cat-clipart-75969.png">
<h1>Fancy Feaster</h1>

<div class="uneaten">
    <ul>
        {{#each burger}}
        {{#unless devoured}}
        {{> burger-block devoured=false}}
        {{/unless}}
        {{/each}}
    </ul>
</div>
<div class="eaten">
      <ul>
        {{#each burger}}
        {{#if devoured}}
        {{> burger-block devoured=true}}
        {{/if}}
        {{/each}}
    </ul>  
</div>



<h2>Add a Flavor</h2>
<form class="create-form">

    <div class="form-group">
        <input type="text" id="ca" name="name">
    </div>
    <br>
    <button type="submit">Add</button>
</form>